        VStack(spacing: 50) {
            Button("BlockOperation") {
                model.addBlockOperation()
            }
            
            Button("SimpleOperation") {
                model.addSimpleOperation()
            }
            
            Button("AsyncOperation") {
                model.addAsyncOperation()
            }
            
            Button("DependencyOperation") {
                model.addDependencyOperation()
            }
        }
    }
